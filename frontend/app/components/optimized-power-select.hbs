<PowerSelect
  ...attributes
  class={{@class}}
  @options={{@options}}
  @placeholder={{@placeholder}}
  @selected={{@selected}}
  @disabled={{@disabled}}
  @allowClear={{@allowClear}}
  @searchEnabled={{true}}
  @searchField={{@searchField}}
  @name={{@name}}
  @tagName={{@tagName}}
  @optionsComponent={{component "optimized-power-select/options"}}
  @triggerComponent={{component "optimized-power-select/trigger"}}
  @onChange={{@onChange}}
  @extra={{this.extra}}
  @onFocus={{this.onFocus}}
  @onKeydown={{this.onKeydown}}
  @dropdownClass={{@dropdownClass}}
/>